<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FButton">
        <attr name="shadowEnabled" format="boolean" />
        <attr name="buttonColor" format="color" />
        <attr name="shadowColor" format="color" />
        <attr name="shadowHeight" format="dimension" />
        <attr name="cornerRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="PianoView">
        <attr name="blackKeyColor" format="color" />
        <attr name="whiteKeyColor" format="color" />
        <attr name="keyPressedColor" format="color" />
        <attr name="blackKeyHeightScale" format="float" />
        <attr name="blackKeyWidthScale" format="float" />
        <attr name="keyCornerRadius" format="dimension" />
        <attr name="keyStrokeWidth" format="dimension" />
        <attr name="keyStrokeColor" format="color" />
        <attr name="numberOfKeys" format="integer" />
    </declare-styleable>

</resources>